{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "preserve",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noEmit": true,
    "alwaysStrict": true,
    "typeRoots" : ["./src/js/types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
